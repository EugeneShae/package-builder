{
    "name": "shae/package-builder",
    "description": "Package templates builder for Laravel framework.",
    "type": "project",
    "authors": [
        {
            "name": "Eugene Shae",
            "email": "evgeniy.shemena@gmail.com"
        },
        {
            "name": "Koterion",
            "email": "koterion317@gmail.com"
        }
    ],
    "license": "Apache-2.0",
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2.10",
        "laravel/framework": "5.8.*",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Shae\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shae\\PackageBuilderServiceProvider"
            ],
            "aliases": {
                "PackageBuilder": "Shae\\PackageBuilderFacade"
            }
        }
    }
}
